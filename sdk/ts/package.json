{
  "name": "@rhinestone/1auth-module",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./rhinestone": {
      "types": "./dist/rhinestone.d.ts",
      "import": "./dist/rhinestone.js"
    }
  },
  "scripts": {
    "build:wasm": "cd ../oneauth && wasm-pack build --target bundler --out-dir ../ts/src/wasm/oneauth && rm -f ../ts/src/wasm/oneauth/.gitignore",
    "build:ts": "tsc",
    "build:copy-wasm": "cp -r src/wasm dist/wasm",
    "build": "npm run build:wasm && npm run build:ts && npm run build:copy-wasm",
    "test": "vitest"
  },
  "dependencies": {
    "viem": "^2.38.0"
  },
  "devDependencies": {
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  }
}
